<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="cm_MyTopBar">
        <attr name="cm_myTopBarTitle" format="string" />
        <attr name="cm_myTopBarLeftIcon" format="reference" />
        <attr name="cm_myTopBarRightIcon" format="reference" />
    </declare-styleable>

    <declare-styleable name="cm_MyFormBar">
        <attr name="cm_myFormBarTitle" format="string" />
        <attr name="cm_myFormBarHint" format="string" />
        <attr name="cm_myFormBarInputType" format="enum">
            <enum name="plainText" value="0" />
            <enum name="password" value="1" />
        </attr>
        <attr name="cm_myFormBarLines" format="integer" />
    </declare-styleable>

    <declare-styleable name="cm_MySignalView">
        <attr name="cm_mySignalLight" format="enum">
            <enum name="green" value="0" />
            <enum name="red" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="cm_MyColumnView">
        <attr name="cm_myColumnNumbers" format="integer" />
        <attr name="cm_myViewState" format="integer" />
    </declare-styleable>

    <declare-styleable name="cm_MyTextCardView">
        <attr name="cm_myTextCardViewText" format="string" />
        <attr name="cm_myTextCardViewTextColor" format="color" />
        <attr name="cm_myTextCardViewTextSize" format="dimension" />
        <attr name="cm_myTextCardViewCardColor" format="color" />
        <attr name="cm_myTextCardViewCardRadius" format="dimension" />
    </declare-styleable>
</resources>