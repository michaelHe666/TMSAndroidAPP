<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <size
                android:width="100dp"
                android:height="100dp" />
        </shape>
    </item>
    <item
        android:bottom="55dp"
        android:left="40dp"
        android:right="30dp"
        android:top="15dp">
        <shape
            android:shape="ring"
            android:thickness="4dp"
            android:useLevel="false">
            <solid android:color="@color/cm_color_universal_foreground" />
        </shape>
    </item>

    <item
        android:bottom="62dp"
        android:left="52dp"
        android:right="37dp"
        android:top="18dp">
        <rotate android:fromDegrees="45">
            <shape android:shape="rectangle">

                <corners
                    android:bottomLeftRadius="8dp"
                    android:bottomRightRadius="8dp" />
                <solid android:color="@color/cm_color_universal_foreground"/>

            </shape>
        </rotate>
    </item>
    <item
        android:bottom="64dp"
        android:left="57dp"
        android:right="38dp"
        android:top="16dp">
        <rotate android:fromDegrees="45">
            <shape android:shape="rectangle">

                <corners
                    android:bottomLeftRadius="4dp"
                    android:bottomRightRadius="4dp" />
                <solid android:color="@color/mm_color_fragment_management_repair" />
            </shape>
        </rotate>
    </item>
    <item
        android:bottom="70dp"
        android:left="64dp"
        android:right="28dp"
        android:top="5dp">
        <rotate android:fromDegrees="-45">
            <shape android:shape="rectangle">

                <solid android:color="@color/mm_color_fragment_management_repair" />
            </shape>
        </rotate>
    </item>

    <item
        android:bottom="22dp"
        android:left="26dp"
        android:right="65dp"
        android:top="33dp">
        <rotate android:fromDegrees="45">
            <shape android:shape="rectangle">
                <corners
                    android:bottomLeftRadius="9dp"
                    android:bottomRightRadius="9dp" />
                <solid android:color="@color/cm_color_universal_foreground" />
            </shape>
        </rotate>
    </item>
    <item
        android:bottom="24dp"
        android:left="32dp"
        android:right="65dp"
        android:top="30dp">

        <rotate android:fromDegrees="45">



            <shape android:shape="rectangle">

                <corners
                    android:bottomLeftRadius="5dp"
                    android:bottomRightRadius="5dp" />
                <solid android:color="@color/mm_color_fragment_management_repair" />
            </shape>
        </rotate>
    </item>

    <item
        android:bottom="35dp"
        android:left="15dp"
        android:right="35dp"
        android:top="15dp">
        <shape
            android:innerRadius="20dp"
            android:shape="ring"
            android:thickness="5dp"
            android:useLevel="false">
            <solid android:color="@color/cm_color_universal_foreground" />
        </shape>
    </item>
    <item
        android:bottom="11dp"
        android:left="64dp"
        android:right="26dp"
        android:top="49dp">
        <rotate android:fromDegrees="-45">
            <shape android:shape="rectangle">
                <size
                    android:width="10dp"
                    android:height="40dp" />
                <corners android:radius="5dp" />
                <solid android:color="@color/cm_color_universal_foreground" />
            </shape>
        </rotate>
    </item>
</layer-list>